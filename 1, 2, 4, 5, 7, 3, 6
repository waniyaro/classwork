#1
a = int(input("Введите первое число = "))
b = int(input("Введите второе число = "))

if a != b:
    if a > b:
        print("1-ое число является максимальным, 2-e минимальным")

    else:
        print("2-ое число является максимальным, 1-е минимальным")

else:
    print("Введенные числа равны")



#2
a = int(input("Введите сторону квадрат = "))
b = int(input("Введите радуис круга = "))

if a < b:
    print("Круг не вместится в квадрат")

if a > b:
    print("Круг вместится в квадрат")

else:
    print("Круг и Квадрат равны")
    
#4
a = int(input("Введите сторону квадрата = "))
b = int(input("Введите радуис круга = "))

if a < b:
    print("Квадрат впишется в круг")

if a > b:
    print("Квадрат не впишется в круг")

else:
    print("Введенные значения равны")
    
#5
a = int(input("Введите первое вещественное = "))
b = int(input("Введите вторео вещественное = "))

if a != b:
    if a > b:
        print("1-ое число является максимальным, 2-e минимальным")

    else:
        print("2-ое число является максимальным, 1-е минимальным")

else:
    print("Введенные числа равны")
    
#7
a = int(input("Введите 1-ое число"))
b = int(input("Введите 2-е число"))

if a < b:
    print("1-e число является минимальным")
    
elif a > b:
    print("2-е число является минимальным")
    
else:
    print("Введенные числа равны")
    
#3
a = int(input("Введи значение для числа 'x' = "))

if a < 0:
    print("y =", a ** 2)

if a > 0:
    print("y =", 1/(a ** 2))

#6
a = int(input("Введи значение для числа 'x' = "))

if a < 0:
    print("y =", a ** 2)

if a > 0:
    print("y =", 1/(a ** 2))
